{
  "name": "travel-management-system-frontend",
  "version": "1.0.0",
  "description": "Comprehensive Angular Frontend for Travel Management System",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssl": "ng serve --ssl",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "docker:build": "docker build -t travel-management/frontend .",
    "docker:run": "docker run -p 4200:80 travel-management/frontend"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/google-maps": "^17.3.0",
    "@angular/material": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/service-worker": "^17.0.0",
    "@stripe/stripe-js": "^8.5.3",
    "chart.js": "^4.5.1",
    "country-state-city": "^3.2.1",
    "date-fns": "^2.30.0",
    "flatpickr": "^4.6.13",
    "jwt-decode": "^4.0.0",
    "ng2-charts": "^4.1.1",
    "ngx-cookie-service": "^17.0.0",
    "ngx-pagination": "^6.0.3",
    "ngx-toastr": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/jasmine": "~5.1.0",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "angular",
    "travel-management",
    "typescript",
    "tailwindcss",
    "material-design"
  ],
  "author": "Travel Management Team",
  "license": "MIT"
}
